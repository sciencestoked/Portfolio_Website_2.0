<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
    <h1>Dashboard</h1>
    <ul>
        <% submissions.forEach(submission => { %>
            <li><strong>Name:</strong> <%= submission.name %> | <strong>Email:</strong> <%= submission.email %> | <strong>Message:</strong> <%= submission.message %></li>
        <% }) %>
    </ul>
</body>
</html>
